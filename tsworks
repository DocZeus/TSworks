import time
import datetime
import pandas as pd

# ticker ='QCOM'
p1 =int(time.mktime(datetime.datetime(2022, 5, 4, 23, 59).timetuple()))
p2 =int(time.mktime(datetime.datetime(2023, 5, 4, 23, 59).timetuple()))
interval='1wk' # 1d, 1m

from configparser import ConfigParser

con = ConfigParser()
con.read("config.ini")
print(con.sections())
print(con['company'])
lis=list(con['company'].values())

query_string='https://query1.finance.yahoo.com/v7/finance/download/{ticker}?period1={p1}&period2={p2}&interval={interval}&events=history&includeAdjustedClose=true'
for i in lis: 
    final_string = query_string.replace("{ticker}", i)
    print(final_string)


# query_string=f'https://query1.finance.yahoo.com/v7/finance/download/{ticker}?period1={period1}&period2={period2}&interval={interval}&events=history&includeAdjustedClose=true'
datf=pd.read_csv(query_string)
print(datf)
datf.to_csv(r'C:\Users\swapn\Python\QCOM.csv')
